{
  "name": "sf-ui",
  "version": "0.1.0",
  "description": "An HTML/CSS/JS UI for visualizing scientific data.",
  "private": true,
  "scripts": {
    "import-wasm": "mkdir -p dist && cp ../lib-cpp/build-wasm/apps/libsf.* dist",
    "test": "APP_VERSION=$npm_package_version jest",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "start": "APP_VERSION=$npm_package_version webpack serve --open",
    "build": "APP_VERSION=$npm_package_version webpack"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/assetMock.ts",
      "\\.(css|less|sass)$": "<rootDir>/src/styleMock.ts",
      "^assets/(.*)": "<rootDir>/src/assets/$1",
      "^components/(.*)": "<rootDir>/src/components/$1",
      "^model/(.*)": "<rootDir>/src/model/$1",
      "^util/(.*)": "<rootDir>/src/util/$1",
      "^worker/(.*)": "<rootDir>/src/worker/$1"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/index.ts"
    ],
    "setupFiles": [
      "<rootDir>/src/jest.stub.ts"
    ]
  },
  "keywords": [
    "sciformats"
  ],
  "author": "Robert Schiwon",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@types/jest": "^29.5.1",
    "@types/plotly.js-dist-min": "^2.3.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "babel-jest": "^29.5.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "html-webpack-plugin": "^5.5.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "style-loader": "^3.3.2",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2",
    "webpack-dev-server": "^4.13.3"
  },
  "dependencies": {
    "plotly.js-dist-min": "^2.22.0"
  },
  "browser": {
    "crypto": false,
    "path": false,
    "fs": false,
    "env": false
  }
}
